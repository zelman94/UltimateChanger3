                        <GradientStop Color="#FF212121"/>
                    </LinearGradientBrush>
                </Grid.Background>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>

                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition Height="285*"/>
                    <RowDefinition Height="150*"/>
                </Grid.RowDefinitions>



