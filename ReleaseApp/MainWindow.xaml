                            <RowDefinition Height="8*"/>
                            <RowDefinition Height="48*"/>
                            <RowDefinition Height="41*"/>
                            <RowDefinition Height="85*"/>
                        </Grid.RowDefinitions>
                        <Button x:Name="btnDeletelogs" Grid.Row="0" Margin="8,-82,12,62" Content="Delete log files" Click="btnDelete_logs" ToolTip="All log files of selected FS will be deleted" IsEnabled="False"/>
