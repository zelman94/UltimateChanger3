                        <DataGridTextColumn Header="Features"  Binding="{Binding Ficzur_}" Width="200"/>
                        <DataGridTextColumn Header="Communication device" Binding="{Binding ComDev_}" Width="*"/>
                    </DataGrid.Columns>

                    <DataGrid.Resources >
                        <Style TargetType="{x:Type DataGridColumnHeader}">
                            <Setter Property="Background" Value="#FF616161" />
                            <Setter Property="FontWeight" Value="SemiBold" />
                            <Setter Property="BorderThickness" Value="0,0,0,2" />
                            <Setter Property="BorderBrush" Value="#FF424242"/>
                            <Setter Property="Foreground" Value="White"/>
                        </Style>
                    </DataGrid.Resources>
                </DataGrid>

                <Button Name="btnExportData" Content="Export" Click="btnExportData_Click" Grid.Column="0" Grid.Row="2" HorizontalAlignment="Right" Background="#FF616161" BorderBrush="#FF424242" Foreground="#E5FFFFFF" Margin="10 0"/>
