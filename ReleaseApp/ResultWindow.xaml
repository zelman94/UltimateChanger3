<Window x:Class="UltimateChanger.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UltimateChanger"
        mc:Ignorable="d"
         Title="Result of randomizer" Height="700" Width="300" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid Background="#212121">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="txtblockResults" Height="651" Margin="10,10" TextWrapping="Wrap" Foreground="White" FontSize="14"/>
    </Grid>
</Window>
